{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "buildCommand": "pip install -r requirements.txt && python manage.py migrate --noinput",
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}

